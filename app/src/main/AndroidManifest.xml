            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegiCardActivity" />
        <activity android:name=".Join" />
        <activity android:name=".Join2" />
    </application>

</manifest>